	<queryString>
		<![CDATA[SELECT
     rekon.`id_pakai` AS rekon_id_pakai,
     rekon.`id_material` AS rekon_id_material,
     rekon.`nm_material` AS rekon_nm_material,
     rekon.`qty_pakai` AS rekon_qty_pakai,
